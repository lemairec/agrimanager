{% extends 'base_agri.html.twig' %}

{% block body %}

<div class="card">
    <div class="card-header">
        <button type="button" class="btn btn-primary" onclick="window.location.href='/lemca/licence/0'">Ajouter</button>
        cameras
    </div>
    <table class="table">
        <tr>
            <th>Licence</th>
            <th>Panel</th>
            <th>Boitier</th>
            <th>Date</th>
            <th>Desc</th>
        </tr>
        {% for licence in licences %}
            <tr>
                <td><a href="/lemca/licence/{{licence.id}}">{{licence.licence}}</a></td>
                <td>{{licence.panel}}</td>
                <td>{{licence.boitier}}</td>
                <td>{{licence.date_create | showDate}}</td>
                <td>{{licence.short_desc}}</td>
            </tr>
        {% endfor %}
    </table>
</div>

{% endblock %}
