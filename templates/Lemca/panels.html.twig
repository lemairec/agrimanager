{% extends 'base_agri.html.twig' %}

{% block body %}

<div class="card">
    <div class="card-header">
        <button type="button" class="btn btn-primary" onclick="window.location.href='/lemca/panel/0'">Ajouter</button>
        Panels
    </div>
    <table class="table">
        <tr>
            <th></th>
            <th>Name</th>
            <th></th>
        </tr>
        {% for panel in panels %}
            <tr>
                <td><a href="/lemca/panel/{{panel.id}}">{{panel.no_fabricant}}</a></td>
                <td>{{panel.generation}}</td>
                <td></td>
            </tr>
        {% endfor %}
    </table>
</div>

{% endblock %}
