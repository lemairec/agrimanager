{% extends 'base_agri.html.twig' %}

{% block body %}

<div class="card">
    <div class="card-header">
        <button type="button" class="btn btn-primary" onclick="window.location.href='/lemca/achat/0'">Ajouter</button>
        Achats
    </div>
    <table class="table">
        <tr>
            <th>Date</th>
            <th>Reference</th>
            <th>Quantity</th>
            <th>Price</th>
        </tr>
        {% for binary in binaries %}
            <tr>
                <td>{{ binary.date | showDate }}</td>
                <td>{{ binary.name }}</td>
                <td>{{ binary.filename }}</td>
                <td></td>
            </tr>
        {% endfor %}
    </table>
</div>

{% endblock %}
