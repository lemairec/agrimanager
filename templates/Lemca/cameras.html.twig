{% extends 'base_agri.html.twig' %}

{% block body %}

<div class="card">
    <div class="card-header">
        <button type="button" class="btn btn-primary" onclick="window.location.href='/lemca/camera/0'">Ajouter</button>
        cameras
    </div>
    <table class="table">
        <tr>
            <th></th>
            <th>Name</th>
            <th></th>
        </tr>
        {% for camera in cameras %}
            <tr>
                <td><a href="/lemca/camera/{{camera.id}}">{{camera.no_fabricant}}</a></td>
                <td>{{camera.ip}}</td>
                <td>{{camera.getKitStr()}}</td>
            </tr>
        {% endfor %}
    </table>
</div>

{% endblock %}
