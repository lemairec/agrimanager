{% extends 'base_agri.html.twig' %}

{% block body %}


    <div class="card">
        <div class="card-header">
            <button type="button" class="btn btn-primary" onclick="window.location.href='{{ url("analyse_sol", {'analyse_sol_id': 0}) }}'">Ajouter</button>
        </div>
        <table class="table">
            <tr>
                <th>Date</th>
                <th>Name</th>
                <th>Ph</th>
                <th>Mo</th>
                <th>P (220)</th>
                <th>K (300)</th>
                <th>Mg (100)</th>

            </tr>
            {% for analyse_sol in analyse_sols %}
            <tr>
                <td><a href="{{ url("analyse_sol", {'analyse_sol_id': analyse_sol.id}) }}"> {{analyse_sol.get_date() }}</td>
                <td>{{analyse_sol.parcelle.campagne}} - {{analyse_sol.parcelle }}</td>
                <td> {{analyse_sol.ph }}</td>
                <td> {{analyse_sol.mo }}</td>
                <td> {{analyse_sol.p }}</td>
                <td> {{analyse_sol.k }}</td>
                <td> {{analyse_sol.mg }}</td>
            </tr>
            {% endfor %}
        </table>
    </div>

{% endblock %}
