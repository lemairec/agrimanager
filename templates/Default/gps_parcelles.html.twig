{% extends 'base_agri.html.twig' %}

{% block body %}

<div class="card">
    <div class="card-header">
        <button type="button" class="btn btn-primary" onclick="window.location.href='/gasoil/0'">Ajouter</button>
        Gps Parcelles
    </div>
    <table class="table">
        <tr>
            <th>name</th>
            <th>update</th>
            <th>surface</th>
        </tr>
        {% for parcelle in parcelles %}
            <tr>
                <td><a href="/gps_parcelle/{{parcelle.name}}">{{ parcelle.name }}</a></td>
                <td>{{ parcelle.datetime | showDatetime }}</td>
                <td>{{ parcelle.surface | showHa() }}</td>
                
                
            </tr>
        {% endfor %}
    </table>


</div>
   
{% endblock %}
