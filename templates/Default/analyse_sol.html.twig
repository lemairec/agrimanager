{% extends 'base_agri.html.twig' %}


{% form_theme form _self %}

{% block form_row %}
<div class="form-group row">
    <div class="col-sm-2">
        {{ form_label(form) }}
    </div>

    <div class="col-sm-10">
        {{ form_widget(form) }}
    </div>
    {{ form_errors(form) }}
</div>
{% endblock form_row %}


{% block body %}

<div class="container bs-docs-container">
    <div class="well">
        {{ form_start(form) }}
        {{ form_row(form.parcelle, {'attr': {'class': 'form-control'}}) }}
        {% if (analyse_sol.docName != '') %}
            <div class="form-group row">
                <div class="col-sm-10">
                    <embed src="{{ vich_uploader_asset(analyse_sol, 'docFile') }}" width="500" height="800" type='application/pdf'>
                </div>
            </div>
        {% else %}
        {% endif %}

        {{ form_end(form) }}
    </div>

</div>
{% endblock %}
