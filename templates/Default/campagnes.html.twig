{% extends 'base_agri.html.twig' %}

{% block body %}

<div class="card">
    <div class="card-header">
        Campagnes
        <button type="button" class="btn btn-primary" onclick="window.location.href='/campagne/0'">{{'Add' | trans }}</button>
    </div>
    <table class="table">
        <tr>
            <th>{{'Name' | trans }}</th>
        </tr>
        {% for campagne in campagnes2 %}
        <tr>
            <td><a href="{{ url("campagne", {'campagne_id': campagne.id}) }}">{{campagne.name}}</a></td>
        </tr>
        {% endfor %}
    </table>
</div>
{% endblock %}
