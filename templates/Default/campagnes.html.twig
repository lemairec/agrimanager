{% extends 'base_agri.html.twig' %}

{% block body %}


<div class="container bs-docs-container">
    <div class="panel panel-default", style="width:100%">
        <!-- Default panel contents -->
        <div class="panel-heading">
            Campagnes
            <button type="button" class="btn btn-primary" onclick="window.location.href='/campagne/0'">{{'Add' | trans }}</button>
        </div>
        <table class="table">
            <tr>
                <th>{{'Name' | trans }}</th>
            </tr>
            {% for campagne in campagnes2 %}
            <tr>
                <td><a href="/campagne/{{campagne.id}}">{{campagne.name}}</a></td>
            </tr>
            {% endfor %}
        </table>
    </div>
</div>
{% endblock %}
