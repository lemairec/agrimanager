{% extends 'base_agri.html.twig' %}

{% block body %}


<div class="card", style="width:100%">
    <div class="card-header">
        Produits Edit
    </div>
    <table class="table">
        <tr>
            <th>{{'Name' | trans}}</th>
            <th>disable</th>
        </tr>
        {% for p in produits %}
            <tr>
                <td>{{ p.name | my_path('produit2', {'produit_id': p.id}) | raw }}</td>
                <td>{{ p.disable }}</td>
                <td><a href="/produit/{{p.id}}/delete">d</a></td>

            </tr>
        {% endfor %}
    </table>
</div>

{% endblock %}
