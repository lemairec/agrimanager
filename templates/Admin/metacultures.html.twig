{% extends 'base_agri.html.twig' %}

{% block body %}

<div class="card">
    <div class="card-header">
        <button type="button" class="btn btn-primary" onclick="window.location.href='{{ url("admin_metaculture", {'id': 0}) }}'">{{'Add' | trans }}</button>

        MetaCulture
    </div>
    <div class="list-group">
        {% for metaculture in metacultures %}
        <div class="list-group-item flex-column align-items-start">
            <div class="row" style="min-height: 1.5rem;">
                <div class="col-md-4">
                    <a href="{{ url("admin_metaculture", {'id': metaculture.id}) }}">{{metaculture.name}}
                    </a>
                </div>
                <div class="col-md-3">
                </div>
                <div class="col-md-5">
                    {{metaculture.cultureUsage}}
                </div>

            </div>

        </div>
        {% endfor %}
    </div>
</div>

{% endblock %}
