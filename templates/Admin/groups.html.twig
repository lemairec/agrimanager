{% extends 'base_agri.html.twig' %}

{% block body %}


<div class="card">
    <table class="table">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>group</th>
        </tr>
        {% for group in groups %}
        <tr>
            <td><a href="{{ url("admin_group", {'id': group.id}) }}">{{group.id}}</td>
            <td>{{group.name}}</td>
        </tr>
        {% endfor %}
    </table>
</div>
{% endblock %}
