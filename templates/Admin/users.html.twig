{% extends 'base_agri.html.twig' %}

{% block body %}


<div class="card">
    <table class="table">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>LastLogin</th>
            <th>Parcelles</th>
            <th>Interventions</th>
            <th>Log</th>
        </tr>
        {% for user in users %}
        <tr>
            <td><a href="{{ url("admin_user", {'id': user.id}) }}">{{user.id}}</td>
            <td>{{user.username}}</td>
            <td>{{user.email}}</td>
            <td>{{user.getLastLogin | showDatetime}}</td>
            <td>{{user.parcelles }}</td>
            <td>{{user.interventions }}</td>
            <td>{{user.logs }}</td>
        </tr>
        {% endfor %}
    </table>
</div>
{% endblock %}
