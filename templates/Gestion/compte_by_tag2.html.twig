{% extends 'base_agri.html.twig' %}

{% block body %}


<div class="card">
    <table class="table">
        <tr>
            <th></th>
            {% for k,t in res2 %}
                <th>{{ t["campagne"] }}</th>
            {% endfor %}
        </tr>

        {% for tag in tags %}
            <tr>
                <td>{{ tag }}</td>
                {% for k,t in res2 %}
                    <td>{{ t["tags"][tag]  | showEur }}</td>
                {% endfor %}
            </tr>
        {% endfor %}

    </table>
</div>

{% endblock %}
