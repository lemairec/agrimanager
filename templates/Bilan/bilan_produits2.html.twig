{% extends 'base_agri.html.twig' %}

{% block body %}
<div class="toto">
    <div class="card">
        <table class="table">
            <tr>
                <th>Culture</th>
                <th>Produit</th>
                {% for campagne in campagnes2 %}
                    <th class="text-right">{{ campagne.name }}</th>
                {% endfor %}
            </tr>
            {% for produit in produits %}
            <tr>
                <td><b>{{produit}}</b></td>
                <td>{{produit.type}}</td>
                {% for campagne in campagnes2 %}
                    <td class="text-right">{{ produit.campagne[campagne.name] | showEurUnity(produit.unity)}}</td>

                {% endfor %}
            </tr>

            {% endfor %}
        </table>
    </div>
</div>
{% endblock %}
