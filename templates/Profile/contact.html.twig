{% extends 'base_form.html.twig' %}

{% block body %}

<div class="card mb-3">
    <div class="card-body">
        {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.text, {'attr': {'rows': '10'}}) }}
        <input type="submit" class="btn btn-primary" value="Envoyer" />
        {{ form_end(form) }}
    </div>

</div>
{% endblock %}
