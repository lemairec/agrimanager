{% extends 'base_agri.html.twig' %}

{% block body %}

<div class="card">
    <div class="card-header">
        Jobs
        <button type="button" class="btn btn-primary" onclick="window.location.href='/robot_job/0'">Ajouter</button>
        
    </div>
    <table class="table">
        <tr>
            <th>Nom</th>
            <th>Type</th>
        </tr>
        {% for job in jobs %}
            <tr>
                <td><a href="/robot_job/{{job.id}}">{{job.name}}</a></td>
                <td>{{job.type}}</td>
                
            </tr>
        {% endfor %}
    </table>
</div>



{% endblock %}
