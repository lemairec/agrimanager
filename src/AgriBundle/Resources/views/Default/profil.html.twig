{% extends 'AgriBundle::base_agri.html.twig' %}



{% form_theme form _self %}

{% block form_row %}
<div class="form-group row">
    <div class="col-sm-2">
        {{ form_label(form) }}
    </div>

    <div class="col-sm-10">
        {{ form_widget(form, {'attr': {'class': 'form-control'}}) }}
    </div>
    {{ form_errors(form) }}
</div>
{% endblock form_row %}


{% block body %}
    <div class="container bs-docs-container">
        <div class="well">
            {{ form(form) }}
        </div>
        <div class="well">
            <a href="http://maplaine.fr/k8f96gtb?user_id={{ app.user.id }}">http://maplaine.fr/k8f96gtb?user_id={{ app.user.id }}</a>
        </div>
    </div>

{% endblock %}
