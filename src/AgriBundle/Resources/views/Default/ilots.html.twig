{% extends 'AgriBundle::base_agri.html.twig' %}

{% block body %}


<div class="container bs-docs-container">
    <div class="panel panel-default", style="width:100%">
        <!-- Default panel contents -->
        <div class="panel-heading">Ilots</div>
        <table class="table">
            <tr>
                <th>Name</th>
                <th>Surface</th>
            </tr>
            {% for ilot in ilots %}
            <tr>
                <td><a href="/ilot/{{ilot.id}}">{{ilot.name}}</a></td>
                <td>{{ilot.surface}}</td>
            </tr>
            {% endfor %}
        </table>
    </div>
</div>
{% endblock %}
